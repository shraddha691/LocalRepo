pipeline {
    agent any
    stages {
        stage('Checkout') {
    steps {
        echo 'Code was already checked out by Jenkins.'
     }
   }
        stage('Build') {
            steps {
                echo 'This is where the build would happen if you had one.'
            }
        }
        stage('Test') {
            steps {
                echo 'Pretend tests are running here.'
            }
        }
    }
}
